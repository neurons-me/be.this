{
  "source": {
    "include": ["./src", "./index.js", "./README.md"]
  },
  "opts": {
    "destination": "./docs",
    "template": "./better-docs", 
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {
      "staticFiles": {
        "include": ["./README.md"]
      }
    },
    "better-docs": {
      "name": "Be.This",
      "title": "Be.This",
      "css": "style.css",
      "trackingCode": "...",
      "hideGenerator": false,
      "navLinks": [
        {
          "label": "Home",
          "href": "https://neurons-me.github.io/"
        },
        {
          "label": "All.This",
          "href": "https://neurons-me.github.io/all.this/"
        },
        {
          "label": "Github",
          "href": "https://github.com/neurons-me"
        },
        {
          "label": ".me",
          "href": "https://cleaker.me"
        }
      ]
    }
  }
}